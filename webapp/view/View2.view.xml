<mvc:View controllerName="masterdetail.masterdetail.controller.View2" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
    <Page>
        <Table id="idProducts" items="{/Suppliers}" inset="false">
            <columns>
                <Column>
                    <Text text="Supplier ID"/>
                </Column>
                
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Text text="{SupplierID}"/>
                        
                    </cells>
                </ColumnListItem>
            </items>


        </Table>

        <Page title="Orders">
                        <List items="{/Suppliers}">
                            <items>
                                <StandardListItem title="{CompanyName}"/>
                            </items>
                        </List>
        </Page>
    </Page>
    </mvc:View>